<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ios_back"
    tools:context=".TabViewFragments.TabVisitUser">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/button_umow_wizyte"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="30dp"
            android:layout_marginRight="30dp"
            android:layout_marginBottom="15dp"
            android:layout_weight="1"
            android:background="@drawable/background_button_round_soft"
            android:onClick="onClickUmowWizyte"
            android:state_pressed="true"
            android:text="Umów wizytę"

            android:textColor="@drawable/button_round_soft_behaviour"
            android:textSize="25dp" />

        <Button
            android:id="@+id/button_moje_wizyty"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="15dp"
            android:layout_marginRight="30dp"
            android:layout_marginBottom="15dp"
            android:layout_weight="1"
            android:background="@drawable/background_button_round_soft"
            android:onClick="onClickMojeWizyty"
            android:text="moje wizyty"
            android:textColor="@drawable/button_round_soft_behaviour"
            android:textSize="25dp"
            tools:elevation="5dp" />

        <Button
            android:id="@+id/button_historia_wizyt"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="15dp"
            android:layout_marginRight="30dp"
            android:layout_marginBottom="30dp"
            android:layout_weight="1"
            android:background="@drawable/background_button_round_soft"
            android:clickable="true"
            android:onClick="onClickHistoriaWizyt"
            android:text="Historia wizyt"
            android:textColor="@drawable/button_round_soft_behaviour"
            android:textSize="25dp" />


    </LinearLayout>
</FrameLayout>